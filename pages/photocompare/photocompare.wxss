/* pages/photocompare/photocompare.wxss */
page {
    height: 100%;
}

.active{
  transform: scale(1.14);
  transition:all .5s ease-in 0s;
  /* z-index: 20;  */
}

.swiper-item{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: flex-start;
  overflow:unset;
}

.slide-image{
  height:90%;
  width: 90%;
  margin-left: 18rpx;
  border-radius:20rpx;
  box-shadow: 0px 10rpx 30rpx rgba(0, 0, 0, .4); 
  /* margin: 0rpx 30rpx; */
  /* z-index: 100;   */
}

.main_separation {
    /* background-color: rgb(246, 246, 246); */
    /* height: 1px; */
    /* margin: 0rpx 25rpx; */
    /* height: 100rpx; */
    /* background: linear-gradient(to top, rgb(109, 102, 102), rgb(255, 255, 255)); */
    /* box-shadow: 0px 10rpx 30rpx rgba(0, 0, 0, .4); */
}

.compare_origin {
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: stretch;
    width: 100%;
    /* height: 100%; */
    position: fixed;
}

.compare_container {
    display: flex;
    flex-direction: row;
    align-items: stretch;
    width: 100%;
    height: 950rpx;
    margin-top: 10rpx;
    /* background: linear-gradient(to top, white, black); */
    /* background: linear-gradient(to right, rgb(252, 48, 97), rgb(252, 90, 65)); */
}

.compare_left {
    /* border: 2rpx solid rgb(241, 241, 241); */
    display: flex;
    width:50%;
    flex-direction: column;
    justify-content: flex-start;
    align-items: stretch;
    /* background: linear-gradient(to top, white, black); */
    /* background:linear-gradient(to right, rgb(252, 48, 97), rgb(252, 90, 65)); */
    border-radius: 20rpx;
    margin-left: 1rpx;
    margin-right: 1rpx;
    /* margin-top: 20px; */
}

.compare_right {
    /* border: 2rpx solid rgb(241, 241, 241); */
    display: flex;
    width:50%;
    flex-direction: column;
    justify-content: flex-start;
    align-items: flex-start;
    margin-left: 1rpx;
    margin-right: 1rpx;
    /* background:linear-gradient(to right, rgb(252, 48, 97), rgb(252, 90, 65)); */
    border-radius: 20rpx;
    /* margin-top: 20px; */
}

.compare_swiper {
    display: flex;
    flex: 0 0 auto;
    height: 800rpx;
    width:100%;
    box-shadow: 0px 10rpx 30rpx rgba(0, 0, 0, .4);
}
.compare_up_1 {
    display: flex;
    flex: 0 0 auto;
    height: 60px;
    width:80%;
}

.compare_up_2 {
    display: flex;
    flex: 0 0 auto;
    height: 60px;
    width:60%;
}

.right_label {
    position: relative;
    border-radius: 6px;
    /* padding: 2rpx; */
    width:60px;
    height: 40rpx;
    line-height: 40rpx;
    bottom: 90rpx;
    left: 0rpx;
    background: linear-gradient(to right, rgb(252, 48, 97), rgb(252, 90, 65));
    color: white;
    font-size: 13px;
    text-align: center;
}

.left_label {
    position: relative;
    border-radius: 6px;
    padding: 2rpx;
    width:60px;
    height: 40rpx;
    line-height: 40rpx;
    bottom: 20rpx;
    left: 0rpx;
    background: linear-gradient(to right, rgb(252, 48, 97), rgb(252, 90, 65));
    color: white;
    font-size: 13px;
    text-align: center;
}

.post_complete_parent {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    width: 100%;
    /* margin-bottom: 100rpx; */
    /* margin-top: 20rpx; */
}

.post_complete {
    border-radius: 30rpx;
    margin: 32rpx;
    font: bold;
    color: white;
    font-size: 35rpx;
    text-align: center;
    height: 70rpx;
    width: 100%;
    line-height: 70rpx;
    /* background:linear-gradient(rgb(253,154,136),rgb(248,0,72)); */
    background:linear-gradient(to right, rgb(252, 48, 97), rgb(252, 90, 65));
}

.post_product_text {
    font: medium;
    font-size: 28rpx;
    margin-bottom: 24rpx;
}

.post_product_image_view {
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-items: center;
    border-radius:  100rpx;
}

.post_product_tag_parent {
    display: flex;
    flex: 1 1 auto;
    flex-direction: row;
    align-items: flex-start;
    flex-wrap: wrap;
    margin: 20rpx;
}

.post_product_tag_sel {
    border-radius: 24rpx;
    padding-left: 18rpx;
    padding-right: 18rpx;
    padding-top: 5rpx;
    padding-bottom: 5rpx;
    margin: 10rpx;
    height: 40rpx;
    line-height: 40rpx;
    font-size: 25rpx;
    text-align: center;
    color:white;
    background:linear-gradient(to right, rgb(252, 48, 97), rgb(252, 90, 65));
}

.post_product_tag {
    border: 2rpx solid rgb(241, 241, 241);
    border-radius: 24rpx;
    padding-left: 16rpx;
    padding-right: 16rpx;
    /* padding-top: 5rpx; */
    /* padding-bottom: 5rpx; */
    margin-left: 10rpx;
    margin-bottom: 10rpx;
    height: 50rpx;
    line-height: 50rpx;
    font-size: 25rpx;
    text-align: center;
    color: rgb(200, 200, 200);
}

.post_skin_tag_parent {
    display: flex;
    flex: 1 1 auto;
    flex-direction: row;
    align-items: flex-start;
    flex-wrap: wrap;
    margin-top: -10rpx;
}

.post_skin_tag {
    border: 1rpx solid rgb(241, 241, 241);
    border-radius: 24rpx;
    padding-left: 10rpx;
    padding-right: 10rpx;
    /* padding-top: 5rpx; */
    /* padding-bottom: 5rpx; */
    margin-left: 10rpx;
    margin-bottom: 10rpx;
    height: 50rpx;
    line-height: 50rpx;
    font-size: 25rpx;
    text-align: center;
    color: rgb(200, 200, 200);
}

.post_product_photo {
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: stretch;
    width: 100%;
    height: 200rpx;
    padding: 16rpx;
}

.left_del {
    flex: 0 0 auto;
    position: relative;
    width:25px;
    height: 25px;
    top: -195px;
    left: 150px;
}