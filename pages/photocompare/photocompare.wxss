/* pages/photocompare/photocompare.wxss */
page {
    height: 100%;
}

.compare_origin {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-items: stretch;
    width: 100%;
    height: 100%;
}

.compare_container {
    display: flex;
    flex-direction: row;
    align-items: stretch;
    width: 100%;
    height: 200px;
    background-color:rgb(255,225,232);

}

.compare_left {
    display: flex;
    width:50%;
    flex-direction: column;
    justify-content: flex-start;
    align-items: stretch;
    margin-top: 20px;
}

.compare_right {
    display: flex;
    width:50%;
    flex-direction: column;
    justify-content: flex-start;
    align-items: flex-start;
    margin-top: 20px;
}

.compare_swiper {
    display: flex;
    flex: 0 0 auto;
    height: 175px;
    width:100%;
}
.compare_up_1 {
    display: flex;
    flex: 0 0 auto;
    height: 60px;
    width:80%;
}

.compare_up_2 {
    display: flex;
    flex: 0 0 auto;
    height: 60px;
    width:60%;
}

.right_label {
    position: relative;
    border-radius: 10px;
    width:60px;
    height: 20px;
    bottom: 25px;
    left: 15px;
    background-color: rgb(255,147,136);
    color: white;
    font-size: 13px;
    text-align: center;
}

.left_label {
    position: relative;
    border-radius: 10px;
    width:60px;
    height: 20px;
    bottom: 25px;
    left: 15px;
    background-color: rgb(255,147,136);
    color: white;
    font-size: 13px;
    text-align: center;
}

.post_complete_parent {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    width: 100%;
    margin-bottom: 40px;
}

.post_complete {
    border-radius: 30rpx;
    margin: 32rpx;
    font: bold;
    color: white;
    font-size: 35rpx;
    text-align: center;
    height: 60rpx;
    width: 100%;
    line-height: 60rpx;
    background:linear-gradient(rgb(253,154,136),rgb(248,0,72));
}